<?xml version="1.0" encoding="utf-8"?>
<androidx.coordinatorlayout.widget.CoordinatorLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.imc.getout.InviteActivity">

    <com.google.android.material.appbar.AppBarLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <FrameLayout
            android:id="@+id/toolbar4"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"

            android:background="#F5F5F5">

            <ImageView
                android:id="@+id/imageView10"
                android:layout_width="100dp"
                android:layout_height="56dp"
                app:srcCompat="@drawable/ahmet" />
        </FrameLayout>

        <com.google.android.material.tabs.TabLayout
            android:id="@+id/inviteTabs"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#fafafa"
            app:tabIndicatorColor="@color/getoutred"
            app:tabSelectedTextColor="@color/getoutred"
            app:tabTextColor="#000" />
    </com.google.android.material.appbar.AppBarLayout>

    <androidx.viewpager.widget.ViewPager
        android:id="@+id/invite_view_pager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
         />

</androidx.coordinatorlayout.widget.CoordinatorLayout>